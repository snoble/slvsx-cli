{
  "schema": "slvs-json/1",
  "units": "mm",
  "parameters": {
    "link1_length": 50.0,
    "link2_length": 80.0,
    "link3_length": 70.0,
    "link4_length": 40.0
  },
  "entities": [
    {
      "type": "point",
      "id": "ground_a",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "ground_b",
      "at": ["$link1_length", 0, 0]
    },
    {
      "type": "point",
      "id": "joint_p",
      "at": [25, 25, 0]
    },
    {
      "type": "point",
      "id": "joint_q",
      "at": [30, 20, 0]
    },
    {
      "type": "line",
      "id": "ground_link",
      "p1": "ground_a",
      "p2": "ground_b"
    },
    {
      "type": "line",
      "id": "input_link",
      "p1": "ground_a",
      "p2": "joint_p"
    },
    {
      "type": "line",
      "id": "coupler",
      "p1": "joint_p",
      "p2": "joint_q"
    },
    {
      "type": "line",
      "id": "output_link",
      "p1": "joint_q",
      "p2": "ground_b"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "ground_a"
    },
    {
      "type": "fixed",
      "entity": "ground_b"
    },
    {
      "type": "distance",
      "between": ["ground_a", "joint_p"],
      "value": "$link2_length"
    },
    {
      "type": "distance",
      "between": ["joint_p", "joint_q"],
      "value": "$link3_length"
    },
    {
      "type": "distance",
      "between": ["joint_q", "ground_b"],
      "value": "$link4_length"
    }
  ]
}