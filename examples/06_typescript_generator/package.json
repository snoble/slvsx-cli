{
  "name": "slvsx-constraint-generator",
  "version": "1.0.0",
  "description": "TypeScript constraint generator for SLVSX",
  "main": "generator.ts",
  "scripts": {
    "generate-types": "json2ts schema.json > types.ts",
    "build": "tsc generator.ts --outDir dist --target ES2020 --moduleResolution node",
    "run": "node dist/generator.js"
  },
  "devDependencies": {
    "json-schema-to-typescript": "^13.1.2",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "dependencies": {}
}