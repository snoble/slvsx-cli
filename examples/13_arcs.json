{
  "schema": "slvs-json/1",
  "units": "mm",
  "description": "Demonstrates arc entity with distance constraints to ensure equal radii",
  "entities": [
    {
      "type": "point",
      "id": "center",
      "at": [50, 50, 0]
    },
    {
      "type": "point",
      "id": "start",
      "at": [100, 50, 0]
    },
    {
      "type": "point",
      "id": "end",
      "at": [50, 100, 0]
    },
    {
      "type": "arc",
      "id": "quarter_circle",
      "center": "center",
      "start": "start",
      "end": "end",
      "normal": [0, 0, 1]
    },
    {
      "type": "line",
      "id": "radius_start",
      "p1": "center",
      "p2": "start"
    },
    {
      "type": "line",
      "id": "radius_end",
      "p1": "center",
      "p2": "end"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "center"
    },
    {
      "type": "distance",
      "between": ["center", "start"],
      "value": 50
    },
    {
      "type": "distance",
      "between": ["center", "end"],
      "value": 50
    }
  ]
}