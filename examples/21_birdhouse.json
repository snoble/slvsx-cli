{
  "schema": "slvs-json/1",
  "units": "mm",
  "description": "A 3D wireframe birdhouse demonstrating geometric constraints",
  "parameters": {
    "width": 150.0,
    "depth": 150.0,
    "wall_height": 180.0,
    "roof_height": 80.0,
    "entrance_diameter": 40.0
  },
  "entities": [
    {"type": "point", "id": "base_fl", "at": [0, 0, 0]},
    {"type": "point", "id": "base_fr", "at": [150, 0, 0]},
    {"type": "point", "id": "base_bl", "at": [0, 150, 0]},
    {"type": "point", "id": "base_br", "at": [150, 150, 0]},
    
    {"type": "point", "id": "wall_fl", "at": [0, 0, 180]},
    {"type": "point", "id": "wall_fr", "at": [150, 0, 180]},
    {"type": "point", "id": "wall_bl", "at": [0, 150, 180]},
    {"type": "point", "id": "wall_br", "at": [150, 150, 180]},
    
    {"type": "point", "id": "ridge_front", "at": [75, 0, 260]},
    {"type": "point", "id": "ridge_back", "at": [75, 150, 260]},
    
    {"type": "line", "id": "base_front", "p1": "base_fl", "p2": "base_fr"},
    {"type": "line", "id": "base_right", "p1": "base_fr", "p2": "base_br"},
    {"type": "line", "id": "base_back", "p1": "base_br", "p2": "base_bl"},
    {"type": "line", "id": "base_left", "p1": "base_bl", "p2": "base_fl"},
    
    {"type": "line", "id": "corner_fl", "p1": "base_fl", "p2": "wall_fl"},
    {"type": "line", "id": "corner_fr", "p1": "base_fr", "p2": "wall_fr"},
    {"type": "line", "id": "corner_bl", "p1": "base_bl", "p2": "wall_bl"},
    {"type": "line", "id": "corner_br", "p1": "base_br", "p2": "wall_br"},
    
    {"type": "line", "id": "wall_front", "p1": "wall_fl", "p2": "wall_fr"},
    {"type": "line", "id": "wall_right", "p1": "wall_fr", "p2": "wall_br"},
    {"type": "line", "id": "wall_back", "p1": "wall_br", "p2": "wall_bl"},
    {"type": "line", "id": "wall_left", "p1": "wall_bl", "p2": "wall_fl"},
    
    {"type": "line", "id": "gable_fl", "p1": "wall_fl", "p2": "ridge_front"},
    {"type": "line", "id": "gable_fr", "p1": "wall_fr", "p2": "ridge_front"},
    {"type": "line", "id": "gable_bl", "p1": "wall_bl", "p2": "ridge_back"},
    {"type": "line", "id": "gable_br", "p1": "wall_br", "p2": "ridge_back"},
    
    {"type": "line", "id": "ridge", "p1": "ridge_front", "p2": "ridge_back"},
    
    {"type": "circle", "id": "entrance", "center": [75, 0, 120], "diameter": "$entrance_diameter"},
    
    {"type": "point", "id": "perch_inner", "at": [75, 0, 90]},
    {"type": "point", "id": "perch_outer", "at": [75, -30, 90]},
    {"type": "line", "id": "perch", "p1": "perch_inner", "p2": "perch_outer"}
  ],
  "constraints": [
    {"type": "fixed", "entity": "base_fl"},
    {"type": "fixed", "entity": "base_fr"},
    {"type": "fixed", "entity": "base_bl"},
    {"type": "fixed", "entity": "base_br"},
    {"type": "fixed", "entity": "wall_fl"},
    {"type": "fixed", "entity": "wall_fr"},
    {"type": "fixed", "entity": "wall_bl"},
    {"type": "fixed", "entity": "wall_br"},
    {"type": "fixed", "entity": "ridge_front"},
    {"type": "fixed", "entity": "ridge_back"},
    {"type": "fixed", "entity": "perch_inner"},
    {"type": "fixed", "entity": "perch_outer"}
  ]
}
