{
  "schema": "slvs-json/1",
  "units": "mm",
  "parameters": {
    "width": 100.0,
    "height": 60.0
  },
  "entities": [
    {
      "type": "point",
      "id": "A",
      "at": [0, 0, 0]
    },
    {
      "type": "point",
      "id": "B",
      "at": [100, 0, 0]
    },
    {
      "type": "point",
      "id": "C",
      "at": [100, 60, 0]
    },
    {
      "type": "point",
      "id": "D",
      "at": [0, 60, 0]
    },
    {
      "type": "line",
      "id": "AB",
      "p1": "A",
      "p2": "B"
    },
    {
      "type": "line",
      "id": "BC",
      "p1": "B",
      "p2": "C"
    },
    {
      "type": "line",
      "id": "CD",
      "p1": "C",
      "p2": "D"
    },
    {
      "type": "line",
      "id": "DA",
      "p1": "D",
      "p2": "A"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "A"
    },
    {
      "type": "horizontal",
      "a": "AB"
    },
    {
      "type": "horizontal",
      "a": "CD"
    },
    {
      "type": "vertical",
      "a": "BC"
    },
    {
      "type": "vertical",
      "a": "DA"
    },
    {
      "type": "distance",
      "between": ["A", "B"],
      "value": "$width"
    },
    {
      "type": "distance",
      "between": ["A", "D"],
      "value": "$height"
    },
    {
      "type": "perpendicular",
      "a": "AB",
      "b": "BC"
    },
    {
      "type": "perpendicular",
      "a": "BC",
      "b": "CD"
    },
    {
      "type": "perpendicular",
      "a": "CD",
      "b": "DA"
    }
  ]
}

