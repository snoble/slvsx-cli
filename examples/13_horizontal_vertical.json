{
  "schema": "slvs-json/1",
  "units": "mm",
  "entities": [
    {
      "type": "plane",
      "id": "xy_plane",
      "origin": [0, 0, 0],
      "normal": [0, 0, 1]
    },
    {
      "type": "point2_d",
      "id": "origin",
      "at": [0, 0],
      "workplane": "xy_plane"
    },
    {
      "type": "point2_d",
      "id": "top",
      "at": [10, 100],
      "workplane": "xy_plane"
    },
    {
      "type": "point2_d",
      "id": "right",
      "at": [100, 10],
      "workplane": "xy_plane"
    },
    {
      "type": "point2_d",
      "id": "corner",
      "at": [100, 100],
      "workplane": "xy_plane"
    },
    {
      "type": "line2_d",
      "id": "vertical_line",
      "p1": "origin",
      "p2": "top",
      "workplane": "xy_plane"
    },
    {
      "type": "line2_d",
      "id": "horizontal_line",
      "p1": "origin",
      "p2": "right",
      "workplane": "xy_plane"
    },
    {
      "type": "line2_d",
      "id": "diagonal",
      "p1": "origin",
      "p2": "corner",
      "workplane": "xy_plane"
    }
  ],
  "constraints": [
    {
      "type": "fixed",
      "entity": "origin",
      "workplane": "xy_plane"
    },
    {
      "type": "vertical",
      "a": "vertical_line",
      "workplane": "xy_plane"
    },
    {
      "type": "horizontal",
      "a": "horizontal_line",
      "workplane": "xy_plane"
    },
    {
      "type": "distance",
      "between": ["origin", "top"],
      "value": 100
    },
    {
      "type": "distance",
      "between": ["origin", "right"],
      "value": 100
    },
    {
      "type": "angle",
      "between": ["horizontal_line", "diagonal"],
      "value": 45
    }
  ]
}
